    "postinstall": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npx eslint --fix",
      "git add"
    ],
    "src/**/*.styled.ts": [
      "npx prettier --write --no-error-on-unmatched-pattern",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
