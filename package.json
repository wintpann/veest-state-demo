    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.3",
    "faker": "5.5.3",
    "husky": "^7.0.1",
    "json-server": "^0.17.0",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.15",
    "prettier": "2.3.2",
    "react-app-rewire-alias": "^1.0.3",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "^5.0.0",
    "typescript": "^4.1.2"
